{
    "name": "effective-potato",
    "version": "9.0.0",
    "description": "this effective potato wants to be a test npm template for future packages",
    "main": "lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "co": "cz",
        "prebuild": "node scripts/encode",
        "build": "node scripts/build",
        "changelog": "node scripts/post-changelog",
        "postinstall": "npx husky install",
        "lint": "eslint --ext .ts .",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "release:minor": "standard-version --release-as minor --first-release",
        "release:patch": "standard-version --release-as patch --first-release",
        "release:major": "standard-version --release-as major --first-release"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/henrycontal/effective-potato.git"
    },
    "keywords": [],
    "author": "HenryContal",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/henrycontal/effective-potato/issues"
    },
    "homepage": "https://github.com/henrycontal/effective-potato#readme",
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "17.6.3",
        "@commitlint/config-conventional": "17.6.3",
        "@digitalroute/cz-conventional-changelog-for-jira": "8.0.1",
        "@types/jest": "29.5.1",
        "@typescript-eslint/eslint-plugin": "5.59.2",
        "@typescript-eslint/parser": "5.59.2",
        "commitlint-config-jira": "1.6.4",
        "commitlint-plugin-jira-rules": "1.6.4",
        "cz-conventional-changelog": "3.3.0",
        "esbuild": "0.17.18",
        "esbuild-plugin-d.ts": "1.1.0",
        "esbuild-plugin-polyfill-node": "0.2.0",
        "eslint": "8.40.0",
        "eslint-config-prettier": "8.8.0",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "standard-version": "9.5.0",
        "ts-jest": "29.1.0",
        "typescript": "5.0.4"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
        }
    },
    "standard-version": {
        "scripts": {
            "postchangelog": "npm run changelog"
        }
    },
    "exports": {
        "import": "./lib/index.js",
        "require": "./lib/cjs.js"
    }
}
