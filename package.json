{
    "name": "effective-potato",
    "version": "0.0.4",
    "description": "this effective potato wants to be a test npm template for future packages",
    "main": "lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "co": "cz",
        "build": "node scripts/build",
        "postinstall": "npx husky install",
        "lint": "eslint --ext .ts .",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/henrycontal/effective-potato.git"
    },
    "keywords": [],
    "author": "HenryContal",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/henrycontal/effective-potato/issues"
    },
    "homepage": "https://github.com/henrycontal/effective-potato#readme",
    "devDependencies": {
        "@commitlint/cli": "17.6.3",
        "@commitlint/config-conventional": "17.6.3",
        "@types/jest": "29.5.1",
        "@typescript-eslint/eslint-plugin": "5.59.2",
        "@typescript-eslint/parser": "5.59.2",
        "cz-conventional-changelog": "3.3.0",
        "esbuild": "0.17.18",
        "esbuild-plugin-d.ts": "1.1.0",
        "eslint": "8.40.0",
        "eslint-config-prettier": "8.8.0",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "ts-jest": "29.1.0",
        "typescript": "5.0.4"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "exports": {
        "import": "./lib/esm.js",
        "require": "./lib/cjs.js"
    }
}
